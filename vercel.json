{
  "version": 2,
  "builds": [
    { "src": "html/**/*.html", "use": "@vercel/static" },
    { "src": "js/**/*", "use": "@vercel/static" },
    { "src": "css/**/*", "use": "@vercel/static" },
    { "src": "images/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/$1" },
    { "src": "/(.*\\.html)", "dest": "/html/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/images/(.*)", "dest": "/images/$1" }
  ]
}
