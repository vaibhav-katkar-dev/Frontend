{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/html/main/index.html" },
    { "src": "/blog", "dest": "/html/main/blog/blog.html" },
    { "src": "/blog/create-lead-generation-form", "dest": "/html/main/blog/create-lead-generation-form.html" },
    { "src": "/blog/custom-online-form-builder", "dest": "/html/main/blog/custom-online-form-builder.html" },
    { "src": "/logo.svg", "dest": "/html/main/logo.svg" },
    { "src": "/((?!.*\\.).*)", "dest": "$1.html" }
  ]
}