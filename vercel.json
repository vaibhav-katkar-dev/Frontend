{
  "version": 2,
  "builds": [
    { "src": "html/**/*.html", "use": "@vercel/static" },
    { "src": "js/**/*", "use": "@vercel/static" },
    { "src": "css/**/*", "use": "@vercel/static" },
    { "src": "images/**/*", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "^(www\\.)form2chat\\.me/(.*)$",
      "status": 308,
      "headers": { "Location": "https://form2chat.me/$2" }
    },
    { "src": "^/$", "dest": "/html/main/index.html" },
    { "src": "^/pricing$", "dest": "/html/main/prise.html" },
    { "src": "^/blog$", "dest": "/html/main/blog/blog.html" },
    { "src": "^/blog/create-lead-generation-form$", "dest": "/html/main/blog/create-lead-generation-form.html" },
    { "src": "^/blog/custom-online-form-builder$", "dest": "/html/main/blog/custom-online-form-builder.html" },
    { "src": "^/blog/form2chat-benefits$", "dest": "/html/main/blog/form2chat-benefits.html" },
    { "src": "^/blog/effective-form-design-tips$", "dest": "/html/main/blog/effective-form-design-tips.html" },
    { "src": "^/logo\\.svg$", "dest": "/html/main/logo.svg" },
    { "src": "^/sitemap\\.xml$", "dest": "/public/sitemap.xml" },
    { "src": "^/robots\\.txt$", "dest": "/public/robots.txt" },
    { "src": "^/js/(.*)$", "dest": "/js/$1" },
    { "src": "^/css/(.*)$", "dest": "/css/$1" },
    { "src": "^/images/(.*)$", "dest": "/images/$1" },
    { "src": "^/(.*)\\.html$", "dest": "/html/$1.html" }
  ]
}
